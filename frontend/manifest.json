{
  "name": "Duo Drive",
  "description": "Your own pair partner for learning",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Duo Drive",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.jpeg",
    "32": "icon32.jpeg",
    "48": "icon48.jpeg",
    "128": "icon128.jpeg"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": ["activeTab"]
}
